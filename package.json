{
    "name": "malevich",
    "version": "0.0.0",
    "main": "src/index.js",
    "scripts": {
        "start": "webpack-dev-server",
        "build": "npm run clean && webpack --env.production && npm run minify",
        "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
        "prettier": "prettier --check \"./src/**/*.ts\"",
        "clean": "rm -rf dist",
        "minify": "npm run minify:html && npm run minify:css",
        "minify:html": "html-minifier -c html-minifier.json -o dist/index.html dist/index.html",
        "minify:css": "csso dist/style.css --output dist/style.css"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/geojson": "^7946.0.7",
        "copy-webpack-plugin": "^5.1.2",
        "csso-cli": "^3.0.0",
        "fork-ts-checker-webpack-plugin": "^3.1.1",
        "html-minifier": "^4.0.0",
        "prettier": "^1.19.1",
        "ts-loader": "^6.2.2",
        "tslint": "^5.20.1",
        "tslint-config-airbnb": "^5.11.2",
        "typescript": "^3.9.7",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "parse-svg-path": "^0.1.2",
        "potrace": "^2.1.8"
    }
}
